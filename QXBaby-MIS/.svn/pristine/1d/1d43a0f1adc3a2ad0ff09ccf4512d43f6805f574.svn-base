   <class name="ProductsMS" table="products" schema="dbo">
        <id name="productID" column="Product_ID">
            <generator class="identity" />
        </id>
        <property name="serial_number" column="serial_number" type="string"/>
        <property name="productCode" column="name" type="string"/>
        <property name="deleted" column="deleted" type="integer"/>
        <property name="unit_id" column="unit1_id" type="integer"/>
        <property name="discount" column="Discount" type="double"/>
        <one-to-one name="price" class="com.onlineMIS.ORM.entity.headQ.SQLServer.PriceMS" lazy="false" cascade="all"/>
        <property name="sizeGroupId" column="SizeGroupID" type="integer"/>
        <property name="colorGroupId" column="ColorGroupID" type="integer"/>
    </class>
    
    <class name="PriceMS" table="price" schema="dbo">
        <id name="productID" column="p_id">
            <generator class="foreign">
                <param name="property">product</param>
            </generator>
        </id>
        <property name="retailprice" column="retailprice" type="double"/>
        <property name="recprice" column="recprice" type="double"/>
        <property name="price1" column="price1" type="double"/>
        <property name="glprice" column="glprice" type="double"/>
        <one-to-one name="product" class="com.onlineMIS.ORM.entity.headQ.SQLServer.ProductsMS"/>
    </class> 
     <class name="BillidxMS" table="billidx" schema="dbo">
        <id name="billid" column="billid">
           <generator class="identity"/>
        </id>
        <property  name="billdate" column="billdate"  type="timestamp"/>
        <property name="billnumber" column="billnumber" type="string"/>
        <property name="a_id" column="a_id" type="integer"/>
        <property name="c_id" column="c_id" type="integer"/>
        <property name="billtype" column="billtype" type="integer"/>
        <property  name="ysmoney" column="ysmoney" type="double"/>
		<property  name="ssmoney" column="ssmoney" type="double"/>
		<property  name="quantity" column="quantity" type="double"/>
		<property  name="jsye" column="jsye" type="double"/>
		<property  name="jsflag" column="jsflag" type="string"/>
		<property  name="note" column="note" type="string"/>
		<property  name="c_araptotal" column="C_ARAPTOTAL" type="double"/>
		<set name="financeBill_Set" cascade="all-delete-orphan" inverse="true" >
             <key column="bill_id"/>
             <one-to-many class="com.onlineMIS.ORM.entity.headQ.SQLServer.FinanceBillMS"/>
        </set>
    </class>  
    <class name="ClientsMS" table="clients" schema="dbo">
        <id name="client_id" column="client_id">
           <generator class="identity"/>
        </id>   
        <property name="deleted" column="deleted" type="boolean"/> 
        <property name="name" column="name" type="string"/>
        <property name="pinyin" column="pinyin" type="string"/>
        <property name="region_id" column="region_id" type="integer"/>
    </class>
    <class name="RegionMS" table="Region" schema="dbo">
         <id name="regionID" column="region_ID" >
           <generator class="identity"/>
        </id>  
        <property name="name" column="name" type="string"/>
        <property name="deleted" column="deleted" type="boolean"/> 
    </class>
    <class name="SaleHistory" table="salepricehis" schema="dbo">
        <composite-id>
             <key-many-to-one name="client" column="c_id" class="com.onlineMIS.ORM.entity.headQ.SQLServer.ClientsMS" lazy="false"/>           
             <key-many-to-one name="product" column="p_id" class="com.onlineMIS.ORM.entity.headQ.SQLServer.ProductsMS" lazy="false"/>  
             <key-property name="unit_id" column="unit_id" type="integer"></key-property>       
        </composite-id>
        <property name="salePrice" column="SalePrice" type="double"/>
        <property name="quantity" column="quantity" type="double"/>
        <property name="discount" column="DISCOUNT" type="double"/>
    </class>

    <class name="FinanceBillMS" table="financebill" schema="dbo">
        <id name="smb_id" column="smb_id">
           <generator class="identity"/>
        </id>   
        <property name="a_id" column="a_id" type="integer"/>
        <property name="c_id" column="c_id" type="integer"/>
        <property name="jftotal" column="jftotal" type="double"/>
        <many-to-one name="billidxMS" column="bill_id" class="com.onlineMIS.ORM.entity.headQ.SQLServer.BillidxMS" not-null="true" lazy="false"/>
    </class>
</hibernate-mapping>